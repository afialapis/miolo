{
  "name": "miolo-demo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "reset": "rm -fr package-lock.json npm-lock.yaml dist/* && npm i",
    "dev": "npx miolo dev --entry=./server/server-dev.mjs",
    "build-client": "npx miolo build-client",
    "build-server": "npx miolo build-server",
    "build": "npm run build-client && npm run build-server",
    "start": "npx miolo start --server-name=miolo_demo_server",
    "stop": "npx miolo stop",
    "restart": "npx miolo restart --server-name=miolo_demo_server",
    "prod": "npm run build && npm run start"
  },
  "type": "module",
  "dependencies": {
    "miolo-react": "file:../miolo-react",
    "intre": "^2.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "6.29.0"
  },
  "devDependencies": {
    "miolo": "../miolo",
    "xeira": "^1.2.3"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "eslintConfig": {
    "extends": [
      "../../node_modules/xeira/configs/eslint.react.mjs"
    ]
  },
  "babel": {
    "extends": "../../node_modules/xeira/configs/babel.react.mjs"
  }
}
