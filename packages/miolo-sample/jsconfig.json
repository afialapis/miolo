{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "#cli/*": ["./cli/*"],
      "#ns/*": ["./ns/*"],
      "#server/*": ["./server/*"],
      "#static/*": ["./static/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}