{
  "name": "miolo-sample",
  "version": "0.0.1",
  "description": "miolo-sample",
  "private": "true",
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "reset": "rm -fr node_modules package-lock.json npm-lock.yaml && npm i",
    "lint": "npx xeira lint src",
    "dev": "npx miolo dev",
    "deb": "npx miolo deb",
    "create-bin": "npx miolo create-bin",
    "build-client": "npx miolo build-client",
    "build-server": "npx miolo build-server",
    "build": "npm run build-client && npm run build-server && npm run create-bin",
    "start": "node ./build/server/run.mjs start 1>> /var/log/afialapis/miolo-sample.log 2>&1",
    "stop": "node ./build/server/run.mjs stop 1>> /var/log/afialapis/miolo-sample.log 2>&1",
    "restart": "node ./build/server/run.mjs restart 1>> /var/log/afialapis/miolo-sample.log 2>&1"
  },
  "author": "Afialapis <info@afialapis.com>",
  "license": "MIT",
  "imports": {
    "#cli/*": "./src/cli/*",
    "#ns/*": "./src/ns/*",
    "#server/*": "./src/server/*",
    "#static/*": "./src/static/*"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stepperize/react": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "farrapa": "^3.0.0-beta.2",
    "intre": "^3.0.0-beta.2",
    "lucide-react": "^0.563.0",
    "miolo-cli": "file:../miolo-cli",
    "miolo-react": "file:../miolo-react",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^6.29.0",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^3.4.0",
    "tinguir": "^0.0.7",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "miolo": "file:../miolo",
    "sass-embedded": "^1.97.3",
    "xeira": "^2.0.0-beta.8"
  }
}
