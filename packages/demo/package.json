{
  "name": "miolo-demo",
  "private": true,
  "description": "miolo demo",
  "main": "run.js",
  "scripts": {
    "cli-build-bundle": "npx xeira bundle --target=browser --source_index=./src/cli/index.js --bundle_folder=./build/js --bundler=rollup",
    "cli-build-trim": "rm -fr build/js/*umd* build/js/*.mjs* build/js/*.min* build/js/*.iife.js*",
    "cli-build-link": "cd build && ln -s ./js/miolo-demo.iife.bundle.js bundle.js",
    "cli-build": "rm -fr build/* && npm run cli-build-bundle && npm run cli-build-trim && npm run cli-build-link",
    "server": "NODE_ENV=development nodemon ./src/server/index.js"
  },
  "author": "Afialapis <info@afialapis.com>",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "miolo-cli-tools": "*",
    "miolo-server-tools": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "7.16.8",
    "@babel/preset-react": "7.16.7",
    "@babel/register": "7.16.9",
    "assign-deep": "^1.0.1",
    "intre": "^1.1.5",
    "miolo": "*",
    "nodemon": "^2.0.22"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react.cjs"
    ]
  },
  "version": "0.2.1"
}
