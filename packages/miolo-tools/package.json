{
  "name": "miolo-tools",
  "version": "0.0.36",
  "description": "cli tools for miolo",
  "main": "lib/index.js",
  "cjs": "dist/miolo-tools.cjs.js",
  "module": "dist/miolo-tools.es.js",
  "browser": "dist/miolo-tools.umd.js",
  "scripts": {
    "lint": "eslint src",
    "lib-clean": "rm -rf lib && mkdir lib",
    "lib-build": "babel src -d lib",
    "lib": "npm run lib-clean && npm run lib-build",
    "dist-clean": "rm -rf dist && mkdir dist",
    "dist-build": "rollup -c",
    "dist": "npm run dist-clean && npm run dist-build",
    "clean": "npm run lib-clean && npm run dist-clean",
    "prepare": "npm run lint && npm run lib && npm run dist"
  },
  "author": "Afialapis <info@afialapis.com>",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "7.16.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "rollup": "^2.67.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
